{
  "name": "@particles/canvas",
  "version": "1.0.0",
  "description": "Canvas-based particle effects system with 20 built-in effects",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./effects": {
      "types": "./dist/effects/index.d.ts",
      "import": "./dist/effects/index.js"
    }
  },
  "files": [
    "dist",
    "demo.html"
  ],
  "scripts": {
    "build": "tsc && node scripts/fix-imports.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "particles",
    "effects",
    "canvas",
    "animation",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
